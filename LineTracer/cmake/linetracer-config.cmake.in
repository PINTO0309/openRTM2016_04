# LineTracer CMake config file
#
# This file sets the following variables:
# LineTracer_FOUND - Always TRUE.
# LineTracer_INCLUDE_DIRS - Directories containing the LineTracer include files.
# LineTracer_IDL_DIRS - Directories containing the LineTracer IDL files.
# LineTracer_LIBRARIES - Libraries needed to use LineTracer.
# LineTracer_DEFINITIONS - Compiler flags for LineTracer.
# LineTracer_VERSION - The version of LineTracer found.
# LineTracer_VERSION_MAJOR - The major version of LineTracer found.
# LineTracer_VERSION_MINOR - The minor version of LineTracer found.
# LineTracer_VERSION_REVISION - The revision version of LineTracer found.
# LineTracer_VERSION_CANDIDATE - The candidate version of LineTracer found.

message(STATUS "Found LineTracer-@PROJECT_VERSION@")
set(LineTracer_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(LineTracer_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(LineTracer_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(LineTracer_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(LineTracer_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(LineTracer_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(LineTracer_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(LineTracer_DEFINITIONS ${<dependency>_DEFINITIONS})

set(LineTracer_VERSION @PROJECT_VERSION@)
set(LineTracer_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(LineTracer_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(LineTracer_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(LineTracer_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

